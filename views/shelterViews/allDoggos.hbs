<header id="userHeader">
  <div>
    <img src="/images/dog.png" alt="dog icon">
    <h1>DOGGO-CONNECT</h1>
  </div>
  <div>
    <a href="/private">Dashbord</a>
    <a href="/logout">Logout</a>
  </div>
</header>

<div id="dogView">
  <a href="/dogs" class="addDogA"><div class="addDog"><p> Add dog</p></div></a>
  {{#each dog}}
    <a href="/dogs/{{this._id}}">
      <div id="dogCard">
        <img src="{{this.imageUrl}}" alt="dog picture">
        <p>{{this.name}}</p>
      </div>
    </a>
  {{/each}}
</div>




